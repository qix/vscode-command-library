{
  "name": "command-library",
  "displayName": "command-library",
  "description": "A set of additional commands for vscode",
  "version": "0.0.1",
  "publisher": "pegvoice",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pegvoice.commander"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "pegvoice.commander",
        "title": "Run command using pegvoice commander"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "test:unit:watch": "jest --watch unit"
  },
  "devDependencies": {
    "@types/node": "^7.0.43",
    "jest": "^29.3.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@types/jest": "^21.1.8",
    "assert": "^1.4.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/src/test/**/*.ts"
    ]
  }
}
